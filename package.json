{
  "name": "cryptodraw-api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "CryptoDraw backend API - Express + TypeScript + PostgreSQL",
  "scripts": {
    "dev": "node --loader ts-node/esm --no-warnings=ExperimentalWarning src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:seed": "node --loader ts-node/esm prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@prisma/client": "^5.6.0",
    "apollo-server-express": "^3.12.1",
    "bull": "^4.12.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ethers": "^6.8.1",
    "express": "^4.19.2",
    "graphql": "^16.8.1",
    "ioredis": "^5.3.2",
    "merkletreejs": "^0.3.11",
    "pg": "^8.11.3",
    "prom-client": "^15.0.0",
    "winston": "^3.11.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bull": "^4.10.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "@types/pg": "^8.10.7",
    "prisma": "^5.6.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
